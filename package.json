{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/main.ts",
  "scripts": {
    "jsstart": "nodemon src/server.js",
    "start:js": "nodemon -L --exitcrash src/server.ts",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "doc": "jsdoc --configure jsdoc.json",
    "tsc": "tsc",
    "start-ts": "nodemon build/server.js",
    "typeorm": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/common/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:live": "rimraf dist && nest build && node dist/src/main"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0 <15"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/platform-fastify": "^8.0.1",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cross-env": "7.0.3",
    "date-and-time": "^1.0.1",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "fastify": "^3.18.1",
    "fastify-swagger": "^4.8.2",
    "jsonwebtoken": "^8.5.1",
    "nest-winston": "^1.5.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "4.1.6",
    "typeorm": "^0.2.34",
    "uuid": "8.3.2",
    "winston": "^3.3.3",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.4",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/date-and-time": "^0.13.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "chai": "4.3.4",
    "debug": "4.3.1",
    "dirty-chai": "2.0.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  }
}
